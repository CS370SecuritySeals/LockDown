<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    tools:context="com.sp18.ssu370.baseprojectapp.FailActivity">

    <ImageView
        android:id="@+id/triangleAccent"
        android:layout_width="128dp"
        android:layout_height="147dp"
        app:srcCompat="@drawable/triangle_accent_t"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/shutdown_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="@string/fail_message"
        android:textAlignment="center"
        android:textColor="@color/colorAccentPurple"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.621" />

    <ImageView
        android:id="@+id/centerLock"
        android:layout_width="125dp"
        android:layout_height="102dp"
        android:layout_marginBottom="148dp"
        android:layout_marginEnd="108dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="144dp"
        app:layout_constraintBottom_toTopOf="@+id/enterLockDown"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/center_lock_t" />

</android.support.constraint.ConstraintLayout>
    <!--android:fontFamily="@font/advent_pro_thin"-->
